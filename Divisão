/******************************************************************************

Sistema necessita de .h depois do nome da biblioteca...

*******************************************************************************/
#include <stdio.h>
#include <stdlib.h>

int main(){
    
    float A,B,Divisao;
    
    scanf("%F %F",&A,&B);
    
    if(B==0){
        printf("Impossivel");
    }
    else{
        Divisao=A/B;
        printf("%F" ,Divisao);
    }

    exit (0);
}

